<!-- Context structure:
	{
		title: "title"
		spaces : [
			{
				moduleID, name, academicYear
			}
		]	
	}-->
<h2>Active Buzz Spaces</h2>
{{#each this.spaces}}
<div>
    <script>
        var t = "{{this.moduleID}}";
        var ref = encodeURI(t);
        var a = '<a href="threads?space=' + ref + '">{{this.moduleID}} - {{this.name}}</a>';
        document.write(a);
    </script>
    <p>Year: {{this.academicYear}}</p>
    <!--TODO Maybe list admins? -->
</div>
{{/each}}