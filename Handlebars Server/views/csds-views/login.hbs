<h1>Login</h1>
<div class="{{messageType}}">
    {{message}}
</div>
<div class="form-group">
    <form id = "csdslogin" action="/submitCSDS" method="post">

            <label for="csUsername">Username: </label>
            <input class="form-control" type="text" id="csUsername" name="csUsername" autofocus="true" required="true" />
            <br/>

            <label for="csPassword">Password: </label>
            <input class="form-control" type="password" id="csPassword" name="csPassword" required="true" />
            <br/>
            <input class="btn btn-primary" type="Submit" value="Login">
    </form>
</div>

