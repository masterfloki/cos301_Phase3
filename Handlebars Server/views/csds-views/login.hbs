<h1>Login</h1>
<div class="alert alert-{{messageType}}">
    {{message}}
</div>

    <form id = "csdslogin" action="/submitCSDS" method="post">
        <div class="form-group">
            <label for="csUsername">Username: </label>
            <input class="form-control" required type="text" id="csUsername" name="csUsername" autofocus="true" required="true" />
        </div>

        <div class="form-group">
            <label for="csPassword">Password: </label>
            <input class="form-control" required type="password" id="csPassword" name="csPassword" required="true" />
        </div>

        <input class="btn btn-primary" type="Submit" value="Login">
    </form>


